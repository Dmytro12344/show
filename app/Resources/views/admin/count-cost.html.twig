{% extends 'admin/index.html.twig' %}

{% block body %}
    <div class="container mt-5">
<table class="table">
    <thead class="thead-dark">
        <tr>
            <th>{{ 'count.table.name' | trans }}</th>
            <th>{{ 'count.table.product_count' | trans }}</th>
            <th>{{ 'count.table.count_price' | trans }}</th>
        </tr>
    </thead>
    <tbody>

        {% for item in result %}
            <tr>
                <td>{{ item.category_name }}</td>
                <td>{{ item.count_id }}</td>
                <td>{{ item.count_price }}</td>
            </tr>
        {% endfor %}

    </tbody>
</table>
    </div>
{% endblock %}